#include<stdio.h>
void main(){

int i,n=4,j,temp;
int data[4]={5, 9, 7, 2};

for(i=0;i<n;i++)
{
  j=i;
  while(j>0&&data[j]<data[j-1])
  {
      temp=data[j];
      data[j]=data[j-1];
      data[j-1]=temp;
      j--;
  }
}
printf("In ascending ordeer :  ");
   for(i=0;i<n;i++)
   {
       printf("%d", data[i]);
   }

}
